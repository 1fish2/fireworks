{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generic schemas",
  "Datetime": {
    "title": "Datetime object"
  },
  "AbsoluteFilePath": {
    "type": "string",
    "pattern": "^(/[^/]+)+$"
  },
  "FilePath": {
    "type": "string",
    "pattern": "^(/?[^/]+)+$"
  },
  "Filename": {
    "type": "string",
    "pattern": "^([^/]+)+$"
  },
  "Identifier": {
    "type": "string",
    "minLength": 1
  },
  "Hostname": {
    "type": "string",
    "format": "hostname"
  },
  "IPAddress": {
    "type": "string",
    "format": "ipv4"
  },
  "LoggingLevel": {
    "enum": [
      "CRITICAL",
      "ERROR",
      "WARNING",
      "INFO",
      "DEBUG",
      "NOTSET"
    ]
  }
}